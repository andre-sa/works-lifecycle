@model wlc2.Models.License

@{
    ViewData["Title"] = "Eliminar Licença";
}

<h1 style="color: green">Eliminar Licença</h1>

<h3>Tem a certeza que deseja eliminar esta licença?</h3>
<div>
    <h4>License</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RcaapCode)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RcaapCode)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar Licença" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
